{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "alexpechkarev/google-maps": "^12.1",
    "bacon/bacon-qr-code": "^3.0",
    "darkaonline/l5-swagger": "^9.0",
    "doctrine/dbal": "^3.1",
    "guzzlehttp/guzzle": "^7.0.1",
    "kreait/laravel-firebase": "^6.1",
    "laravel/framework": "^11.0",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.10",
    "lcobucci/jwt": "^5.2",
    "pragmarx/google2fa": "^8.0",
    "promphp/prometheus_client_php": "^2.14",
    "stripe/stripe-php": "^10.0"
  },
  "require-dev": {
    "spatie/laravel-ignition": "^2.0",
    "fakerphp/faker": "^1.24",
    "friendsofphp/php-cs-fixer": "^3.82",
    "knuckleswtf/scribe": "^5.2",
    "laravel/breeze": "^2.3",
    "laravel/sail": "^1.43",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.8",
    "larastan/larastan": "^3.5",
    "phpunit/phpunit": "^11.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Chatify\\": "packages/munafio/chatify/src"
    },
    "files": [],
    "exclude-from-classmap": [
      "vendor/munafio/chatify/src/ChatifyMessenger.php",
      "vendor/munafio/chatify/src/ChatifyServiceProvider.php",
      "vendor/munafio/chatify/src/Http/Models/Message.php",
      "vendor/munafio/chatify/src/Http/Models/Favorite.php",
      "vendor/munafio/chatify/src/Http/Controllers/MessagesController.php",
      "vendor/munafio/chatify/src/Facades/ChatifyMessenger.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Tests\\Integration\\": "tests/Integration/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "lint": [
      "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
    ],
    "lint:fix": [
      "@lint:cs",
      "@lint:phpstan"
    ],
    "lint:phpstan": [
      "./vendor/bin/phpstan analyse"
    ],
    "lint:cs": [
      "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
    ]
  }
}
