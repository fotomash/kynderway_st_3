{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "alexpechkarev/google-maps": "^12.0",
        "darkaonline/l5-swagger": "^8.0",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/sanctum": "^2.15",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "munafio/chatify": "^1.0",

    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.14",
        "knuckleswtf/scribe": "^4.22",
        "laravel/breeze": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Chatify\\": "packages/munafio/chatify/src"
        },
        "files": [],
        "exclude-from-classmap": [
            "vendor/munafio/chatify/src/ChatifyMessenger.php",
            "vendor/munafio/chatify/src/ChatifyServiceProvider.php",
            "vendor/munafio/chatify/src/Http/Models/Message.php",
            "vendor/munafio/chatify/src/Http/Models/Favorite.php",
            "vendor/munafio/chatify/src/Http/Controllers/MessagesController.php",
            "vendor/munafio/chatify/src/Facades/ChatifyMessenger.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": [
            "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
        ],
        "lint:fix": [
            "@lint:cs",
            "@lint:phpstan"
        ],
        "lint:phpstan": [
            "./vendor/bin/phpstan analyse"
        ],
        "lint:cs": [
            "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
        ]
    }
}
